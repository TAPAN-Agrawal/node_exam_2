<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../Table.css">
</head>
<body>
    <div>
      <a href="/serviceForm"><button>Add service</button></a>
    </div>
    <table>
        <thead>
          <tr>
           
            <th> Customer Name</th>
            <th>Vehicle Number</th>
            <th>Service Price</th>
            <th>Payble Amount</th>
           <th>action</th>
    
    
    
          </tr>
        </thead>
        <tbody>
          <% users.forEach(function(user) { %>
            <tr>
             
              <td><%= user.customerName %></td>
              <td><%= user.vehicleNumber %></td>
              <td><%= user.servicePrice %></td>
              <td><%= user.payableAmount %></td>


               <td>
                <a href="/serviceUpdateList/<%= user.id %>"><button>Edit</button></a>

                
                <a href="/serviceDelete/<%= user.id %>"><button>Delete</button></a>
               </td>
    
            </tr>
          <% }); %>
        </tbody>
      </table>
</body>
</html>